				<key>script</key>
				<string>set filetype to (system attribute "filetype")
set content to "{query}"

-- resolve home folder
set working_folder to do shell script "echo " &amp; quoted form of (system attribute "working_folder") &amp; " | iconv -f UTF-8-MAC -t MACROMAN"set AppleScript's text item delimiters to "~/"set theTextItems to every text item of working_folderset AppleScript's text item delimiters to (POSIX path of (path to home folder as string))set resolved_path to theTextItems as string

-- prepend certain shebangs
if (filetype is "js") then 
	set content to "#!/usr/bin/env osascript -l JavaScript" &amp;  "\n" &amp; content
else if (filetype is "applescript")
	set content to "#!/usr/bin/env osascript" &amp;  "\n" &amp; content
else if (filetype is "sh")
	set content to "#!/bin/zsh" &amp;  "\n" &amp; content 
end if

-- generate random name
set filename to ((random number from 100000 to 999999) as text)

-- create document
tell application "BBEdit"
	activate
	set newDoc to make new document
	set contents of newDoc to content
	set file2save to (resolved_path &amp; "/" &amp; filename &amp; "." &amp; filetype)
	save newDoc to file2save
end tell</string>
